CREATE TABLE USER (
USER_ID INT,
NAME VARCHAR2(20),
USERNAME VARCHAR2(20),
PASSWORD VARCHAR2(20),
ROLE VARCHAR2(20)
)

CREATE TABLE ARTICLE (
  ARTICLE_ID       INT,
  TITLE            VARCHAR2,
  AUTHOR           VARCHAR2(20),
  DATE             DATE,
  SUMMARY          VARCHAR2(1000),
  FULL_ARTICLE VARCHAR2(10000),
  ISLEADINGARTICLE BOOLEAN
)

CREATE TABLE PICTURE(
ARTICLE_ID  INT,
URL VARCHAR2(20)
)
